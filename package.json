{
  "name": "produtivus",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run serve",
  "build:css": "sass --quiet-deps --style=expanded --no-source-map css/scss/app.scss css/app.css",
  "build:css:prod": "sass --quiet-deps --style=compressed --no-source-map css/scss/app.scss css/app.css",
  "watch:css": "sass --quiet-deps --watch --style=expanded --embed-source-map --embed-sources css/scss/app.scss:css/app.css",
  "watch:css:poll": "sass --quiet-deps --watch --poll --style=expanded --embed-source-map --embed-sources css/scss/app.scss:css/app.css",
  "build:app-css": "sass --quiet-deps --style=expanded --embed-source-map --embed-sources css/scss/app.scss css/app.css",
  "build:app-css:prod": "sass --quiet-deps --style=compressed --no-source-map css/scss/app.scss css/app.css",
    "serve": "http-server -c-1 . -p 8081",
    "build:mindmaps": "npm --prefix apps/mindmaps run build",
    "dev:mindmaps": "npm --prefix apps/mindmaps run dev",
  "build:sheets": "npm --prefix apps/sheets run build",
  "dev:sheets": "npm --prefix apps/sheets run dev",
    "build:all": "npm-run-all -s build:ocsss build:css build:mindmaps",
    "build:ocsss": "sass --no-source-map packages/ocsss/src/index.scss packages/ocsss/dist/index.css --style=expanded",
    "build:ocsss:prod": "sass packages/ocsss/src/index.scss packages/ocsss/dist/index.css --style=compressed --source-map",
    "watch:ocsss": "sass --watch packages/ocsss/src/index.scss:packages/ocsss/dist/index.css --style=expanded",
  "build": "npm-run-all -s build:ocsss build:css",
    "reinstall:fullcalendar": "node js/reinstall-fullcalendar.js",
    "test:cadernos:e2e": "node tools/tests/cadernos-e2e.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Silveirabrazil/Produtivus-3.0.0.0.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Silveirabrazil/Produtivus-3.0.0.0/issues"
  },
  "homepage": "https://github.com/Silveirabrazil/Produtivus-3.0.0.0#readme",
  "description": "",
  "dependencies": {
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.5.0",
    "fullcalendar": "^6.1.19"
  },
  "devDependencies": {
    "bootstrap": "^5.3.3",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^24.19.0",
    "sass": "^1.92.0",
    "ttf2woff": "^2.0.2",
    "ttf2woff2": "^4.0.5"
  }
}
